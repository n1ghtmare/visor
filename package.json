{
    "name": "visor",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "npm run migrate-dev && next dev",
        "build": "npm run migrate-prod && next build",
        "start": "next start -p $PORT",
        "lint": "next lint",
        "migrate-prod": "node-pg-migrate up --tsconfig ./tsconfig.json --envPath ./.env.production -t migrations --rejectUnauthorized false",
        "migrate-dev": "node-pg-migrate up --tsconfig ./tsconfig.json --envPath ./.env.development -t migrations"
    },
    "dependencies": {
        "@tippyjs/react": "^4.2.5",
        "bcrypt": "^5.0.1",
        "date-fns": "^2.22.1",
        "next": "latest",
        "next-iron-session": "^4.2.0",
        "node-pg-migrate": "^5.10.0",
        "pg": "^8.7.1",
        "react": "^17.0.2",
        "react-colorful": "^5.3.0",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.9.0",
        "react-markdown": "^7.0.0",
        "swr": "^0.5.6",
        "uuid": "^3.4.0"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.3.3",
        "@types/bcrypt": "^5.0.0",
        "@types/node-pg-migrate": "^2.3.1",
        "@types/pg": "^8.6.1",
        "@types/react": "^17.0.11",
        "@types/react-dom": "^17.0.9",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "autoprefixer": "^10.2.6",
        "dotenv": "^10.0.0",
        "eslint-config-next": "^11.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "postcss": "^8.3.5",
        "tailwindcss": "^2.2.4",
        "ts-node": "^8.8.1",
        "typescript": "^4.3.5"
    }
}
